<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="itemData"
            type="com.taichi.prompts.android.repository.data.UserProfileMatchVOList" />
    </data>

    <androidx.cardview.widget.CardView
        android:id="@+id/itemCard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_10"
        app:cardCornerRadius="@dimen/dp_20">

        <RelativeLayout
            android:id="@+id/frame_984"
            android:layout_width="335dp"
            android:layout_height="497dp"
            android:background="@drawable/framecard">
            <RelativeLayout
                android:id="@+id/frame_16244"
                android:layout_width="295dp"
                android:layout_height="433dp"
                android:layout_centerInParent="true">
                <TextView
                    android:id="@+id/question"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:text="你最喜欢的哲学家是？"
                    android:textSize="20sp"
                    android:textColor="#161916"
                    android:textAppearance="@style/some_id"
                    android:gravity="top"
                    />

                <androidx.cardview.widget.CardView
                    android:id="@+id/jake_blucke"
                    android:layout_width="295dp"
                    android:layout_height="295.12dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="51.66dp"
                    app:cardCornerRadius="16dp">
                    <ImageView
                        android:id="@+id/headImg"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />
                    <RelativeLayout
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        android:id="@+id/frame_semi"
                        android:layout_width="107dp"
                        android:layout_height="22dp"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="20dp"
                        android:layout_alignParentTop="true"
                        android:layout_marginTop="20.99dp"
                        android:background="@drawable/frame_simi">
                        <View
                            android:id="@+id/ellipse_67"
                            android:layout_width="4dp"
                            android:layout_height="4dp"
                            android:background="#BDCF99"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="10dp"
                            />
                        <TextView
                            android:id="@+id/similar"
                            android:layout_width="73dp"
                            android:layout_height="16dp"
                            android:text="80%共同兴趣点"
                            android:textAppearance="@style/some_id"
                            android:lineSpacingExtra="3sp"
                            android:textSize="10sp"
                            android:textColor="#FFFFFF"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@id/ellipse_67"
                            android:layout_marginLeft="@dimen/dp_5"
                            />
                    </RelativeLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <TextView
                            android:id="@+id/answer"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintLeft_toLeftOf="parent"
                            android:layout_marginLeft="20dp"
                            app:layout_constraintRight_toRightOf="parent"
                            android:layout_marginRight="20dp"
                            android:lineSpacingExtra="3sp"
                            android:text="哈哈哈哈哈只要不是亚里士多德这个SB就行"
                            android:textColor="#000000"
                            android:textSize="20sp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:padding="20dp"
                            android:layout_marginBottom="20dp" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>
                <RelativeLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:id="@+id/frame_9d84"
                    android:layout_width="205dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="1dp"
                    android:layout_height="62dp">
                    <RelativeLayout
                        android:id="@+id/frame_91184"
                        android:layout_width="205dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true">
                        <TextView
                            android:id="@+id/userNickName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="宇宙无敌小白龙"
                            android:textAppearance="@style/some_id"
                            android:textColor="#161916"
                            android:textSize="20sp"
                            android:gravity="top"
                            />
                        <RelativeLayout
                            xmlns:android="http://schemas.android.com/apk/res/android"
                            android:id="@+id/frame"
                            android:layout_width="18dp"
                            android:layout_height="18dp"
                            android:clipToOutline="true"
                            android:layout_centerInParent="true"
                            android:layout_toRightOf="@+id/nickname"
                            android:layout_marginLeft="@dimen/dp_5"
                            android:background="@drawable/veri"
                            />


                    </RelativeLayout>


                    <RelativeLayout
                        android:id="@+id/frame_220"
                        android:layout_width="183dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true">
                        <RelativeLayout
                            xmlns:android="http://schemas.android.com/apk/res/android"
                            android:id="@+id/frame_17"
                            android:layout_width="wrap_content"
                            android:layout_height="24dp"
                            android:background="@drawable/frame_17">
                            <TextView
                                android:id="@+id/mbti"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="infp"
                                android:textAppearance="@style/some_id"
                                android:textColor="#FFFFFF"
                                android:textSize="10sp"
                                android:layout_centerInParent="true"
                                />

                        </RelativeLayout>

                        <RelativeLayout
                            xmlns:android="http://schemas.android.com/apk/res/android"
                            android:id="@+id/frame_21"
                            android:layout_width="wrap_content"
                            android:layout_height="24dp"
                            android:layout_toRightOf="@id/frame_17"
                            android:layout_marginLeft="@dimen/dp_2"
                            android:background="@drawable/frame_21">
                            <TextView
                                android:id="@+id/slender"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="50kg/165cm"
                                android:textAppearance="@style/some_id"
                                android:textColor="#FFFFFF"
                                android:textSize="10sp"
                                android:layout_centerInParent="true"
                                />

                        </RelativeLayout>

                        <RelativeLayout
                            xmlns:android="http://schemas.android.com/apk/res/android"
                            android:id="@+id/frame_20"
                            android:layout_width="wrap_content"
                            android:layout_height="24dp"
                            android:layout_toRightOf="@id/frame_21"
                            android:layout_marginLeft="@dimen/dp_2"
                            android:background="@drawable/frame_20">
                            <TextView
                                android:id="@+id/city"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="杭州"
                                android:textAppearance="@style/some_id"
                                android:textColor="#FFFFFF"
                                android:textSize="10sp"
                                android:layout_centerInParent="true"
                                />
                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>

            </RelativeLayout>

        </RelativeLayout>

    </androidx.cardview.widget.CardView>

</layout>


